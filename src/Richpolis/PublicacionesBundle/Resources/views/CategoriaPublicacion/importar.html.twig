{% block mainbar %}
<!-- Main bar -->
<div class="mainbar">

    <!-- Page heading -->
    <div class="page-head">
        <h2 class="pull-left">Exportar registros</h2>
        <div class="clearfix"></div>
        <!-- Breadcrumb -->
        <div class="bread-crumb">
            <a href="{{path('backend_homepage')}}"><i class="fa fa-home"></i>Dashboard</a> 
            <!-- Divider -->
            <span class="divider">/</span> 
            <a href="{{path('categorias_publicaciones')}}" class="bread-current">Categorias</a>
            <span class="divider">/</span> 
            <a href="{{path('categorias_publicaciones_importar')}}" class="bread-current">Importar</a>
        </div>

        <div class="clearfix"></div>

    </div>
    <!-- Page heading ends -->


    <!-- Matter -->

    <div class="matter">
        <div class="container">

            <div class="row">

                <div class="col-md-12">

                    <div class="widget">
                        <div class="widget-head">
                            <div class="pull-left">Importar</div>
                            <div class="clearfix"></div>
                        </div>

                        <div class="widget-content">
                            <div class="padd">

                                <!-- Profile form -->

                                <div class="form profile">
                                    <!-- Edit profile form (not working)-->
                                    <form class="form-horizontal" action="{{path('categorias_publicaciones_importar')}}" method="post" enctype="multipart/form-data">
                                        <div id="formulario_importar" class="row" style="padding-left: 20px;">
                                        <!-- Checkbox -->
                                        <div class="form-group col-lg-8 col-lg-offset-1 right">
                                            <label for="archivo" class="required">Archivo</label>
                                            <input type="file" id="archivo" name="archivo" required="required" class="form-control placeholder" placeholder="Archivo" data-bind="value: archivo">
                                        </div>
                                        <!-- Buttons -->
                                        <div class="form-group">
                                            <!-- Buttons -->
                                            <div class="col-lg-6 col-lg-offset-1">
                                                <button type="submit" class="btn btn-success">Importar</button>
                                            </div>
                                        </div>
                                        </div>
                                    </form>
                                </div>

                            </div>
                        </div>
                        <div class="widget-foot">
                            <br/>
                        </div>    
                    </div>  

                </div>

            </div>

        </div>
    </div>

    <!-- Matter ends -->

</div>

